{
  "name": "react-starter",
  "version": "1.1.1",
  "repository": "https://gitlab.zengenti.com/starter-projects/react-starter",
  "author": "http://zengenti.com",
  "license": "None",
  "keywords": [],
  "description": "",
  "main": "index.js",
  "config": {
    "startup": ""
  },
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack serve --config webpack/webpack.config.dev.js",
    "build": "rimraf ./dist && cross-env NODE_ENV=production webpack --config webpack/webpack.config.prod.js && npm run build:startup",
    "build:assets": "cross-env NODE_ENV=production webpack --config webpack/webpack.config.assets.js",
    "build:ci": "npm run build",
    "build:startup": "buildstartup",
    "server": "node --max-http-header-size=800000 dist/start.js",
    "dev:server": "cross-env NODE_ENV=test NODE_OPTIONS=--max-http-header-size=80000 babel-node -x .ts,.tsx,.js,.jsx --inspect=9229 src/server/debug.ts",
    "debug:server": "nodemon --delay 500ms --watch \"src/**\" --ext \"ts,tsx,js,jsx,json\" --signal SIGTERM --exec \"npx kill-port 3001 && npm run dev:server\"",
    "build:server": "npm run build && npm run server",
    "build:dev:server": "npm run build && npm run dev:server",
    "analyze": "rimraf ./dist && cross-env NODE_ENV=production ANALYZE=TRUE webpack --config webpack/webpack.config.prod.js",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "test:generate-output": "jest --json --outputFile=.jest-test-results.json || true",
    "test": "jest  -c ./config/jest.config.js --detectOpenHandles --forceExit",
    "test:watch": "jest --config=./config/jest.config.js --watch",
    "test:coverage": "jest --config=./config/jest.config.js --coverage",
    "prebuild:storybook": "npm run test:generate-output",
    "storybook": "storybook dev -p 6006",
    "storybook-ci": "storybook dev --ci -p 6006",
    "storybook-static": "storybook build -c .storybook -o .out",
    "build-storybook": "storybook build"
  },
  "browserslist": [
    "last 2 Chrome versions, last 2 Edge versions, last 2 Firefox versions, last 2 Safari versions, last 2 iOS versions"
  ],
  "overrides": {
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.24.8",
    "@babel/core": "^7.25.2",
    "@babel/eslint-parser": "^7.25.1",
    "@babel/node": "^7.25.0",
    "@babel/preset-env": "^7.25.3",
    "@babel/preset-react": "^7.24.7",
    "@babel/preset-typescript": "^7.24.7",
    "@babel/runtime": "^7.25.0",
    "@loadable/babel-plugin": "^5.16.1",
    "@loadable/webpack-plugin": "^5.15.2",
    "@storybook/addon-a11y": "^8.2.9",
    "@storybook/addon-actions": "^8.2.9",
    "@storybook/addon-essentials": "^8.2.9",
    "@storybook/addon-links": "^8.2.9",
    "@storybook/addon-webpack5-compiler-babel": "^3.0.3",
    "@storybook/react": "^8.2.9",
    "@storybook/react-webpack5": "^8.2.9",
    "@types/chai": "^4.3.16",
    "@types/express": "^4.17.13",
    "@types/loadable__component": "^5.13.9",
    "@types/mocha": "^10.0.7",
    "@types/react-helmet": "^6.1.11",
    "@types/react-redux": "^7.1.33",
    "@types/react-router-config": "^5.0.11",
    "@types/react-router-dom": "^5.3.3",
    "@types/react-router-hash-link": "^2.4.5",
    "@types/styled-components": "^5.1.34",
    "@typescript-eslint/eslint-plugin": "^5.20.0",
    "@typescript-eslint/parser": "^5.20.0",
    "babel-loader": "^9.1.3",
    "babel-plugin-dynamic-import-node": "^2.3.3",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-plugin-styled-components": "^2.1.4",
    "babel-plugin-transform-define": "^1.0.1",
    "chai": "^4.3.6",
    "chai-http": "^4.3.0",
    "copy-webpack-plugin": "^12.0.2",
    "core-js": "^3.37.1",
    "cross-env": "^7.0.3",
    "css-loader": "^7.1.2",
    "esbuild-loader": "^4.2.2",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-import-resolver-webpack": "^0.13.4",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.9.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.3",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-storybook": "^0.8.0",
    "eslint-webpack-plugin": "^4.2.0",
