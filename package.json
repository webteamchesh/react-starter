    "mocha": "^10.4.0",
    "nodemon": "^2.0.15",
    "prettier": "^3.3.2",
    "storybook": "^8.2.9",
    "style-loader": "^4.0.0",
    "typescript": "^4.9.5",
    "webpack": "^5.92.1",
    "webpack-bundle-analyzer": "^4.10.2",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.4",
    "webpack-merge": "^5.9.0",
    "webpack-modules": "^1.0.0",
    "webpack-node-externals": "^3.0.0",
    "zengenti-buildstartup-package": "^0.0.18"
  },
  "dependencies": {
    "@contensis/canvas-react": "^1.1.0",
    "@reduxjs/toolkit": "^2.2.5",
    "@zengenti/contensis-react-base": "^3.2.3-beta.0",
    "@contensis/forms": "^1.0.0-beta.5",
    "app-root-path": "^3.0.0",
    "react-router-hash-link": "^2.4.3",
    "sitemap": "^8.0.0"
  }
}
